<snippet>
	<content><![CDATA[// If there is an ordering column and this is a new row then get the next ordering value
if (property_exists(\$this, 'ordering') && \$this->id == 0)
{
	\$this->ordering = self::getNextOrder();
}

]]></content>
	<tabTrigger>jadmintablecheckordering</tabTrigger>
</snippet>
